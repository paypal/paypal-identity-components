{
  "name": "@paypal/identity-components",
  "version": "5.0.13",
  "description": "PayPal Identity components, for integrating auth products.",
  "main": "index.js",
  "scripts": {
    "dev": "babel-node $(npm bin)/webpack-dev-server --config webpack.config.dev.js --port 9001 --host localhost.paypal.com --open demo/dev/index.htm --https --hot=false",
    "lint": "npm run format:check && eslint --ext .js --ext .jsx src/ test/ *.js",
    "flow-typed": "rm -rf flow-typed && flow-typed install",
    "flow": "flow",
    "karma": "cross-env NODE_ENV=test babel-node $(npm bin)/karma start",
    "test": "if [ ! $SKIP_TEST ]; then npm run lint && npm run flow-typed && npm run flow && npm run jest-ssr && npm run karma; fi;",
    "webpack": "babel-node $(npm bin)/webpack --progress",
    "build": "npm run test && npm run webpack",
    "release": "./scripts/publish.sh",
    "release:patch": "./scripts/publish.sh patch",
    "release:minor": "./scripts/publish.sh minor",
    "release:major": "./scripts/publish.sh major",
    "preversion": "./scripts/preversion.sh",
    "version": "./scripts/version.sh",
    "postversion": "./scripts/postversion.sh",
    "clean": "rimraf dist coverage",
    "reinstall": "rimraf flow-typed && rimraf node_modules && npm install && flow-typed install",
    "debug": "cross-env NODE_ENV=debug",
    "demo": "serve ./demo -l 1337",
    "eslint-find-rules": "eslint-find-rules --current .eslintrc.js --unused --plugin",
    "jest-screenshot": "jest test/screenshot --env=node --no-cache",
    "jest-ssr": "jest test/ssr --env=node --no-cache",
    "format": "prettier --write ./",
    "format:check": "npx prettier --check ./"
  },
  "files": [
    "src/",
    "dist/",
    "__sdk__.js",
    "globals.js"
  ],
  "browserslist": [
    "IE >= 11",
    "chrome >= 27",
    "firefox >= 30",
    "safari >= 5",
    "opera >= 23"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/paypal/paypal-identity-components.git"
  },
  "homepage": "https://developer.paypal.com/",
  "keywords": [
    "cross-domain",
    "cross domain",
    "components",
    "component",
    "krakenjs",
    "kraken"
  ],
  "license": "Apache-2.0",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@krakenjs/grumbler-scripts": "^8.0.4",
    "@krakenjs/sync-browser-mocks": "^3.0.0",
    "conventional-changelog-cli": "^2.0.11",
    "cross-env": "^7.0.3",
    "flow-bin": "0.155.0",
    "flow-typed": "^3.8.0",
    "fs-extra": "^4.0.2",
    "imagemagick": "^0.1.3",
    "imgur": "^0.2.1",
    "jest": "^29.3.1",
    "memory-fs": "^0.4.1",
    "mocha": "^4",
    "mocketeer": "^0.3.0",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.3.0",
    "prettier": "^2.7.1",
    "puppeteer": "^1.14.0",
    "serve": "^10.1.2"
  },
  "dependencies": {
    "@paypal/common-components": "^1.0.33",
    "@paypal/funding-components": "^1.0.27",
    "@paypal/sdk-client": "^4.0.166",
    "@paypal/sdk-constants": "^1.0.57",
    "@paypal/sdk-logos": "^2.0.0",
    "@krakenjs/belter": "^2.0.0",
    "@krakenjs/cross-domain-utils": "^3.0.0",
    "@krakenjs/jsx-pragmatic": "^3.0.0",
    "@krakenjs/post-robot": "^11.0.0",
    "@krakenjs/zalgo-promise": "^2.0.0",
    "@krakenjs/zoid": "^10.0.0"
  }
}
